    "/api/authentication": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "DateOfBirth",
                  "Email",
                  "FirstName",
                  "Gender",
                  "LastName",
                  "Password",
                  "PhoneNumber",
                  "UserName"
                ],
                "type": "object",
                "properties": {
                  "FirstName": {
                    "maxLength": 100,
                    "pattern": "^[A-Za-zء-ي\\s'-]+$",
                    "type": "string"
                  },
                  "LastName": {
                    "maxLength": 100,
                    "pattern": "^[A-Za-zء-ي\\s'-]+$",
                    "type": "string"
                  },
                  "UserName": {
                    "maxLength": 50,
                    "type": "string"
                  },
                  "Password": {
                    "pattern": "^(?=.*[A-Z])(?=.*\\d)(?=.*[@$#!%*?&])[A-Za-z\\d@$#!%*?&]{8,}$",
                    "type": "string"
                  },
                  "Email": {
                    "type": "string",
                    "format": "email"
                  },
                  "PhoneNumber": {
                    "type": "string",
                    "format": "tel"
                  },
                  "ProfilePicture": {
                    "type": "string",
                    "format": "binary"
                  },
                  "DateOfBirth": {
                    "type": "string",
                    "format": "date"
                  },
                  "Gender": {
                    "$ref": "#/components/schemas/Gender"
                  }
                }
              },
              "encoding": {
                "FirstName": {
                  "style": "form"
                },
                "LastName": {
                  "style": "form"
                },
                "UserName": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                },
                "Email": {
                  "style": "form"
                },
                "PhoneNumber": {
                  "style": "form"
                },
                "ProfilePicture": {
                  "style": "form"
                },
                "DateOfBirth": {
                  "style": "form"
                },
                "Gender": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
